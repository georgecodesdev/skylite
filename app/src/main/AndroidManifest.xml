<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skylite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/skl_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.ActivityTrophy"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.ActivityConstellation"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.ActivityConstellationInfo"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activities.ActivityCalendar"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>